        app:layout_constraintBottom_toBottomOf="@+id/textViewNameTextEdit1"
        app:layout_constraintStart_toEndOf="@+id/textViewNameTextEdit1"
        app:layout_constraintTop_toTopOf="@+id/textViewNameTextEdit1"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textViewNameToEditNameAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
