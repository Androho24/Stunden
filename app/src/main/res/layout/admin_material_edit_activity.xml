        app:layout_constraintStart_toStartOf="@+id/textViewNewUnitEditMatNameAdmin"
        app:layout_constraintTop_toBottomOf="@+id/textViewNewUnitEditMatNameAdmin" />

    <EditText
        android:id="@+id/editTextNewNameEditMatListAdmin"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        app:layout_constraintBottom_toBottomOf="@+id/textViewNewNameNameEditMatListAdmin"
        app:layout_constraintEnd_toEndOf="@+id/buttonSaveEditMatListAdmin"
        app:layout_constraintStart_toStartOf="@+id/spinnerNewUnitEditMatListAdmin"
        app:layout_constraintTop_toTopOf="@+id/textViewNewNameNameEditMatListAdmin" />

    <Button
        android:id="@+id/buttonDeleteEditMatListAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
